@model IEnumerable<Triggmine.Report.Scheduler.Models.Customer>

@{
    ViewData["Title"] = "About";   

}


<link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/v/dt/dt-1.10.18/datatables.min.css" />


<table id="table_id">
        <thead>
            <tr>
                
                <th>
                    @Html.DisplayNameFor(model => model.UserName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.FirstName)
                </th>               
              
              
              
                <th>
                    @Html.DisplayNameFor(model => model.Email)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ShopUrl)
                </th>
                         
               
              
                <th>
                    @Html.DisplayNameFor(model => model.RegistrationDate)
                </th>                
                
                
                
                <th>
                    @Html.DisplayNameFor(model => model.LastSingInDate)
                </th>
                
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    
                    <td>
                        @Html.DisplayFor(modelItem => item.UserName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.FirstName)
                    </td>                   
                  
                   
                    <td>
                        @Html.DisplayFor(modelItem => item.Email)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ShopUrl)
                    </td> 
                    <td>
                        @Html.DisplayFor(modelItem => item.RegistrationDate)
                    </td>                  
                   
                    <td>
                        @Html.DisplayFor(modelItem => item.LastSingInDate)
                    </td>
                    
                </tr>
            }
        </tbody>
    </table>




<script>

    $(document).ready(function () {
        $('#table_id').DataTable();
    });
</script>